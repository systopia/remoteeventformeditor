<?xml version="1.0"?>
<extension key="remoteeventformeditor" type="module">
  <file>remoteeventformeditor</file>
  <name>Event Form Editor (CiviRemote)</name>
  <description>Form editor for event registration with CiviRemote</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA GmbH</author>
    <email>info@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/remoteeventformeditor</url>
    <url desc="Documentation">https://github.com/systopia/remoteeventformeditor</url>
    <url desc="Support">https://github.com/systopia/remoteeventformeditor/issues</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate/>
  <version>1.1.0-dev</version>
  <develStage>dev</develStage>
  <compatibility>
    <ver>5.54</ver>
  </compatibility>
  <comments/>
  <requires>
    <ext>de.systopia.remoteevent</ext>
  </requires>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <civix>
    <namespace>CRM/RemoteEventFormEditor</namespace>
    <format>23.02.1</format>
    <angularModule>crmRemoteEventFormEditor</angularModule>
  </civix>
  <mixins>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>ang-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
    <mixin>entity-types-php@1.0.0</mixin>
    <mixin>mgd-php@1.0.0</mixin>
  </mixins>
  <upgrader>CRM_RemoteEventFormEditor_Upgrader</upgrader>
</extension>
